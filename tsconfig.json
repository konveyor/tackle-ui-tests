{
    "compilerOptions": {
        "baseUrl": ".",

        "target": "es5",
        "lib": ["es5", "dom", "es2015"],
        "types": [
            "node",
            "cy-verify-downloads",
            "cypress",
            "cypress-file-upload",
            "cypress-react-selector",
            "cypress-real-events",
            "@testing-library/cypress",
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "sourceMap": true
    },
    "typedocOptions": {
        "entryPoints": ["cypress/integration"],
        "out": "docs"
    },
    "include": [
        "**/*.ts",
        // "**/*.js",
        "../node_modules/dom-accessibility-api/dist/polyfills/SetLike.d.ts"
    ],
    // "exclude": ["../node_modules"]
}
