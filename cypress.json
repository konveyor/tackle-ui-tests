{
    "viewportWidth": 1920,
    "viewportHeight": 1080,
    "video": false,
    "env": {
        "user": "admin",
        "pass": "Dog8code",
        "git_user": "",
        "git_password": "",
        "tackleUrl": "https://tackle-konveyor-tackle.apps.mgn02.cnv-qe.rhcloud.com"
    },
    "retries": {
        "runMode": 2,
        "openMode": 0
    },
    "reporter": "cypress-multi-reporters",
